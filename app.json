{
  "expo": {
    "name": "my-calc",
    "slug": "my-calc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "scheme": "myapp",
    "jsEngine": "hermes",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FF7F00"
      },
      "package": "com.vizn.calculadora"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon-1024.png",
      "bundleIdentifier": "com.vizn.my-calc"
    },
    "extra": {
      "eas": {
        "projectId": "6f2d1c8a-f4a0-4e74-b00f-3cc751ace2e4"
      },
      "router": {}
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/my-calc"
    }
  }
}
